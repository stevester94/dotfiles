[global_config]
[keybindings]
  new_tab = <Primary>t
  next_tab = <Primary><Shift>Right
  prev_tab = <Primary><Shift>Left
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    background_darkness = 0.86
    background_type = transparent
    copy_on_selection = True
    cursor_color = "#aaaaaa"
    scrollback_infinite = True
